{
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "query",
          "key": "v",
          "value": "(?!nuclear).*"
        }
      ],
      "destination": "/?v=nuclear",
      "permanent": false
    },
    {
      "source": "/",
      "missing": [
        {
          "type": "query",
          "key": "v"
        }
      ],
      "destination": "/?v=nuclear",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    },
    {
      "source": "/workbox-*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}